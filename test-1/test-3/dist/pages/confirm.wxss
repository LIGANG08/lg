
.container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.menue{
  position: fixed;
  bottom: 0px;
  border:1px solid silver;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.price{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 60%;
  height: 30px;
  border: 1px solid silver;
  /* flex: 1; */
  padding: 5px;
  font-size: 15px;
}
.corder{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 40%;
  height: 30px;
  border: 1px solid silver;
  background-color: orangered;
  /* flex: 1; */
  padding: 5px;
  font-size: 15px;
}

