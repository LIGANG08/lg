
  <view class="container">
    <view class="photo">
    <view class="f_goods">
      <view class="fgoods"  wx:for="{{fresh}}" wx:key="index">
        <image class="f_pic" src="{{item.f_pic}}" id="{{index}}" bindtap="lagoods"/>
        <text class="f_introduce">{{item.f_int}}</text>
        <text class="f_price">{{item.f_price}}</text>
        <text class="f_number">{{item.f_number}}</text>
        <image class="f_collect" src="{{item.acollect ? '../images/bcollect.png' : notcollect}}" id="{{index}}" bindtap="collect"/>
      </view>
     </view> 
  </view>
  </view>